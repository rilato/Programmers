#include <string>
#include <vector>
#include <algorithm>

using namespace std;

int solution(int k, int m, vector<int> score) {
    int answer = 0;
    int cnt = 0;
    
    sort(score.begin(), score.end(), greater <int>());
    
    for (int i = 0; i < score.size(); i++) {
        cnt++;
        
        if (cnt == m) {
            answer += m * score[i];
            cnt = 0;
        }
    }
    
    return answer;
}
